#include<stdio.h>
int main(){
int hr,min;
char t[5];
scanf("%d:&d %s",&hr,&min,&t);
if (t[0]=='A'&&1<=hr<=11)
    printf("Equivalent 24-hour time :%2d:%2d",hr,min );
else if (t[0]=='P'&&1<=hr<=11)
    printf("Equivalent 24-hour time :%2d:%2d",hr,min );
else if (t[0]=='A'&&hr==12)
    printf("Equivalent 24-hour time :00%2d ",min);
}

